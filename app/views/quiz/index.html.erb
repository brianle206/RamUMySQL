<h1>Quiz#index</h1>
<p>Find me in app/views/quiz/index.html.erb</p>
<% @courses.each do |course| %>
	<li><%= course.title %></li>
		<% course.quizzes.each do |quiz| %>
			<%=link_to quiz.title, edit_quiz_path(quiz) %>
			<%=link_to "Destroy", "/quiz/#{quiz.id}", :method => "delete" %>
		<% end %>
		<li><%= link_to "Create Quiz For this course", "/learn/#{course.id}/quiz/new" %></li>
<% end %>
